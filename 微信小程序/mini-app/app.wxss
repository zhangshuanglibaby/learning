/* 引入字体图标 */
@import './styles/iconfont.wxss';

/* 初始化 */
page,view,text,image,navigator,swiper,swiper-item,button,input {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

/* 图片宽百分比 */
image {
    width: 100%;
}

page {
    color: #666;
    font-size: 28rpx;
    background-color: #eee;
    /* 声明变量 */
    --themeColor : #ff9e1f;
}